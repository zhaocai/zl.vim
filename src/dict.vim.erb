" ============================================================================
" True Or False:                                                          [[[1
" ============================================================================
function! <%=zl%>#dict#has_key_value(dict, key, val) "                         [[[2
    if !has_key(a:dict,a:key)
        return 0
    endif
    if get(a:dict,a:key) != a:val
        return 0
    endif
    return 1
endfunction


